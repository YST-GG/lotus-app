         //输入用户名时判断
    function CheckName(){
        var name_input=$('#name').val()
        //console.log("name_input",name_input)
        var name_data="";
        //用ajax去数据库匹配，
        $.ajax({
            url:"http://47.100.10.123:10080/pool/get_pool_user", //要处理的页面
