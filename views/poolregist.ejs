            type:"GET",  //提交方式
            dataType:"json", //返回的数据类型，TEXT字符串 JSON返回JSON XML返回XML；dataType中T要大写！！
            success: function(json){  //回调函数，data为形参，是从login-cl.php页面返回的值
                var i=0;
                for(i;json.data.length>=i ;i++){
                    //console.log("name_data",name_data)
                    name_data=json.data[i].name
                    console.log("name_data",name_data);
                    if(name_input === name_data){
                        $("#ts").html("*该用户名已注册*");
                        $("#ts").css("color","red");
                        return;
                    }else if(name_input===null|| name_input===''){
                        $("#ts").html("*用户名不能为空*");
